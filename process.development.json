{
	"apps": [{
		"name": "webapi",
		"cwd": "/app/audience",
		"script": "build/src/app.js",
		"watch": true,
		"ignore_watch": [
			"log",
			"files",
			"uploads"
		],
		"env": {
			"NODE_ENV": "development"
		},
		"env_production": {
			"NODE_ENV": "production"
		}
	},{
		"name": "syncAudienceWebapi",
		"cwd": "/app/audience",
		"script": "build/src/modules/bin/sync/sync.js",
		"watch": true,
		"env_production": {
			"NODE_ENV": "production"
		},
		"env": {
			"NODE_ENV": "development"
		}
	}]
}